# zsh configuration file of dyne:bolic GNU/Linux
# with the full multimedia environment
# by jaromil http://rastasoft.org

export HOSTTYPE="i586"
export LANG="C"
export MACHTYPE="i586-pc-linux-gnu"
export OSTYPE="linux-gnu"
export SHELL="/bin/zsh"
	
if [ -z $DYNE_MODULES_SOURCED ]; then # setup base paths

    export PATH=/usr/bin:/usr/sbin:/bin:/sbin:/usr/X11R6/bin:.
    export MANPATH=/usr/share/man:/usr/netpbm/man
    export INFOPATH=/usr/share/info
    export PKG_CONFIG_PATH=/usr/X11R6/lib/pkgconfig
    export LOCATE_PATH=/usr/var/lib/updatedb

fi

export PAGER="/usr/bin/most"
export EDITOR="/usr/bin/edit"
export LIBIND_PATH="/usr/lib/libind"
export XWINHOME="/usr/X11R6"
export X11HOME="$XWINHOME"
export QTDIR="/usr"


# A lucky prompt:
PROMPT='[d:b] %B%30<..<%~%b %(!.#.>)'

umask 022

# the terminal definition
TERM="xterm-color"


