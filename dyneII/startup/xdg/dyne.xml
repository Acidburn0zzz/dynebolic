<?xml version="1.0"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
  <mime-type type="application/x-kernel-module">
    <comment>Kernel driver module</comment>
    <glob pattern="*.ko"/>
  </mime-type>
  <mime-type type="application/x-freej-script">
    <comment>FreeJ realtime video script</comment>
    <glob pattern="*.freej"/>
  </mime-type>
  <mime-type type="application/x-dyne-module">
    <comment>Dyne modular software collection</comment>
    <glob pattern="*.dyne"/>
  </mime-type>
  <mime-type type="application/x-windows-driver">
    <comment>Windows device driver</comment>
    <glob pattern="*.inf"/>
  </mime-type>
  <mime-type type="text/x-tbt">
    <comment>Time Based Text</comment>
    <glob pattern="*.tbt"/>
  </mime-type>
  <mime-type type="application/x-win32-exe">
    <comment>Windows executable</comment>
    <glob pattern="*.EXE"/>
  </mime-type>
  <mime-type type="application/x-win32-drv">
    <comment>Windows device driver</comment>
    <glob pattern="*.INF"/>
  </mime-type>
  <mime-type type="application/x-dyne-nest">
    <comment>Dyne nest for user settings and data</comment>
    <glob pattern="*.nst"/>
  </mime-type>
  <mime-type type="application/x-dyne-kernel">
    <comment>Dyne Linux kernel</comment>
    <glob pattern="*.krn"/>
  </mime-type>
  <mime-type type="application/x-dyne-kernel-modules">
    <comment>Dyne Linux kernel modules</comment>
    <glob pattern="*.kmods"/>
  </mime-type>
  <mime-type type="application/x-dyne-nest-key">
    <comment>Dyne nest encryption key</comment>
    <glob pattern="*.nst.gpg"/>
  </mime-type>


</mime-info>
