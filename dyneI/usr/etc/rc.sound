#!/bin/sh

FILE_ID="\$Id$"
source /lib/libdyne.sh

# check if alsa drivers are loaded
ISALSA=`lsmod | grep -i snd-`

if [ ! -z "$ISALSA" ]; then
  notice "loading alsa-oss emulation modules"
  loadmod snd-pcm-oss
  loadmod snd-mixer-oss
fi

act "Initializing sound levels"
if [ -e /dev/mixer ]; then
    aumix -v 80 -w 80 -i 80
 fi

